{
  "name": "byjg/swagger-test",
  "description": "A set of tools for testing your REST calls based on the Swagger or OpenAPI documentation using PHPUnit",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "byjg/webrequest": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "byjg/restserver": "^5.0",
    "vimeo/psalm": "^5.9"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\ApiTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
