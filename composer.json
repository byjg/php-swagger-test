{
  "name": "byjg/swagger-test",
  "description": "A set of tools for test your REST calls based on the swagger documentation using PHPUnit",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "João Gilberto Magalhães",
      "email": "joao@byjg.com.br"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "6.*",
    "byjg/uri": "1.0.*",
    "php": ">=5.6",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*",
    "byjg/restserver": "3.0.*"
  },
  "autoload": {
    "psr-4": {
      "ByJG\\ApiTools\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  }
}
